    <input id="new_post_title" class="defaultText" value="Where are you in Yale?" alt="Where are you in Yale?"
      name="post[title]"/>
    
    <button id="submit_post">Submit </button>

  </div>
  
  <div class="postbox" style="display:none">
    <h2 class="post_body"></h2>
    <span class="post_title"></span>
    
    <span class="post_time"></span>
  </div>



  
</div>
<div id="footer">&copy; Circa: Yale 2012 </div>
  <div id="positions">
